{
    "name": "bfc-vscode-snippets",
    "displayName": "BFC-VSCode-Snippets",
    "description": "VS Code snippets for Bot Framework devs",
    "version": "0.0.2",
    "icon": "assets/bfc_icon.png",
    "publisher": "BotFrameworkCommunity",
    "engines": {
        "vscode": "^1.43.0"
    },
    "categories": [
        "Snippets"
    ],
    "keywords": [
        "Bot Framework",
        "Web Chat",
        "LUIS"
    ],
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/BotBuilderCommunity/botbuilder-community-tools"
    },
    "contributes": {
        "languages": [{
            "id": "lu",
            "aliases": ["lu"],
            "extensions": [".lu"]
        }],
        "snippets": [
            {
                "language": "html",
                "path": "./snippets/snippets.json"
            },
            {
                "language": "lu",
                "path": "./snippets/lu-snippets.json"
            },
            {
                "language": "json",
                "path": "./snippets/ac-snippets.json"
            }
        ]
    }
}